extends ../pug/layout.pug
//- mixins
include ../blocks/breadcrumbs/breadcrumbs.pug
include ../blocks/block-sidebar-menu/sidebar-menu.pug
include ../blocks/sidebar-advantages/sidebar-advantages.pug
include ../blocks/video/video.pug


block vars
	- var page = 'news-item';
	- var header_class = "main-header main-header--bg" 
	- var menu_btn_class = "btn-white main-header__menu-btn"

block content
	+breadcrumbs([{href:"#",name: "Главная"},{href:"#",name: "О нас"},{href:"#",name: "Новости"}, {href: "#", name: "Открытие нового центра в г. Тюмени", class: "current" }])

	section.page.page-news-item
		.container
					h2.page-title Открытие нового центра в г. Тюмени
					.news-detail
						.news-detail__date
						.page-content
							p Имеется спорная точка зрения, гласящая примерно следующее: явные признаки победы институционализации смешаны с не уникальными данными до степени совершенной неузнаваемости, из-за чего возрастает их статус бесполезности. Предварительные выводы неутешительны...
						.gallery
							.gallery-slider.js-gallery-slider
								div.gallery-slider__item
									img(src="/dist/img/programm_desc/baner_bg.jpg", alt="")
								div.gallery-slider__item
									img(src="/dist/img/programm_desc/baner_bg2.jpg", alt="")
								div.gallery-slider__item
									img(src="/dist/img/programm_desc/baner_bg3.jpg", alt="")
								div.gallery-slider__item
									img(src="/dist/img/programm_desc/baner_bg4.jpg", alt="")
								div.gallery-slider__item
									img(src="/dist/img/programm_desc/baner_bg.jpg", alt="")
